<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Lista de Identificadores</title>
</head>
<body>
  <h1>Lista de Identificadores</h1>
  <ul>
    <% identificadores.forEach(identificador => { %>
      <li>
        <%= identificador.nombre %>
        <form action="/eliminar/<%= identificador.id %>" method="post">
          <button type="submit">Eliminar</button>
        </form>
      </li>
    <% }); %>
  </ul>
  <form action="/agregar" method="post">
    <input type="text" name="nuevoIdentificador" placeholder="Nuevo Identificador">
    <button type="submit">Agregar</button>
  </form>
</body>
</html>
